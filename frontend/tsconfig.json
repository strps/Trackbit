{
  // "files": [],
  "compilerOptions": {
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "target": "ES2020",
    "noEmit": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src",
    "../types/*"
  ]
}